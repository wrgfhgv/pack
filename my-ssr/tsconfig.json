{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": false,
        "outDir": "./dist",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
    },
    "include": ["frontEnd/**/*", "server/**/*"],
    "exclude": ["node_modules", "dist"]
}